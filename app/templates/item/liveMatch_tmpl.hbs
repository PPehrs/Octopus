<div class="fz-2 c-p octopus_liveMatch_container home-match-container clearfix">
	<a style="float:left" href="javascript:;"><i class="fa fa-info-circle"></i></a>

	{{#if isEncounter}}
		<div class="m-l-b flex"    style="border:1px dotted;border-radius:5px;font-weight: normal;margin-bottom: 5px;">
			<div>{{home}}</div>
			<div class="m-l-s m-r-s">vs.</div>
			<div>{{guest}}</div>
			<div class="m-l">{{homeSets}}</div>
			<div>:</div>
			<div>{{guestSets}}</div>
		</div>
	{{/if}}

	<div class="m-l-b flex">
		<div>{{p1Name}}</div>
		<div class="m-l-s m-r-s">vs.</div>
		<div>{{p2Name}}</div>
        <div class="m-l">{{p1Legs}}</div>
        <div>:</div>
        <div>{{p2Legs}}</div>
    </div>
</div>


